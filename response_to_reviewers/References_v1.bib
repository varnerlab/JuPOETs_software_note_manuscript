	Mesh = {Computer Simulation; Escherichia coli; Escherichia coli Proteins; Kinetics; Metabolic Clearance Rate; Metabolic Flux Analysis; Metabolome; Models, Biological; Signal Transduction},
	Month = {Sep},
	Pages = {63-71},
	Pmid = {24972370},
